@model IEnumerable<PrimeiroMVC.Models.Aluno >
@{
    ViewData["Title"] = "Listar Aluno";
}
<h1>Listar Alunos</h1>

<table>
    <thead>
        <tr>

            <th>Id</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Matricula</th>

        </tr>

    <tbody>

            @foreach (var aluno in Model)
            {
            <tr>

                <td>@aluno.AlunoId</td>
                <td>@aluno.Nome</td>
                <td>@aluno.Email</td>
                <td>@aluno.RM</td>

            </tr>
            }


    </tbody>

    </thead>
</table>